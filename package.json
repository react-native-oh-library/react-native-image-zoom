{
  "name": "@react-native-oh-tpl/react-native-image-pan-zoom",
  "version": "2.1.12-0.0.1",
  "description": "react native image pan zoom",
  "main": "lib/commonjs/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "scripts": {
    "bob": "bob build",
